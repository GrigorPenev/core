{
    "glueAssets": {
        "gateway": {
            "location": "./glue/gateway-web.js"
        },
        "worker": "./glue/worker.js",
        "config": "./glue.config.json",
        "route": "/glue"
    },
    "server": {
        "settings": {
            "port": 4242,
            "disableCache": true
        },
        "apps": [
            {
                "route": "/dummyApp",
                "file": {
                    "path": "./dummyApp"
                }
            }
        ],
        "sharedAssets": [
            {
                "route": "/glue/glue.layouts.json",
                "path": "./glue/glue.layouts.json"
            },
            {
                "route": "/glue/workspaces/",
                "path": "./glue/workspaces"
            }
        ]
    },
    "logging": "default"
}